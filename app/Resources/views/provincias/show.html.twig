{% extends 'base.html.twig' %}

{% block body %}
    <h1>Provincias</h1>

    <table>
        <tbody>
            <tr>
                <th>Idprv</th>
                <td>{{ provincia.idPrv }}</td>
            </tr>
            <tr>
                <th>Codigoprv</th>
                <td>{{ provincia.codigoPrv }}</td>
            </tr>
            <tr>
                <th>Nombreprv</th>
                <td>{{ provincia.nombrePrv }}</td>
            </tr>
            <tr>
                <th>Ordenprv</th>
                <td>{{ provincia.ordenPrv }}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('provincias_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('provincias_edit', { 'id': provincia.idPrv }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
